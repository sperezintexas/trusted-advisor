# Copy to .env and set values. Do not commit .env.
# For Atlas: use mongodb+srv://user:pass@cluster.mongodb.net/trusted-advisor (include DB name in path)
# or set MONGODB_DATABASE if your URI has no path.
MONGODB_URI=mongodb://localhost:27017/trusted-advisor
MONGODB_DATABASE=trusted-advisor
XAI_API_KEY=
AUTH_SEED_ENABLED=true
AUTH_SEED_USER_ID=default
AUTH_SEED_PROVIDER=x
AUTH_SEED_PROVIDER_USER_ID=x-seed-user-001
AUTH_SEED_PROVIDER_USERNAME=trusted_advisor_x_seed
